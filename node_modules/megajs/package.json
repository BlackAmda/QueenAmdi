{
  "name": "megajs",
  "version": "1.1.4",
  "description": "Unofficial JavaScript SDK for MEGA",
  "main": "dist/main.node-cjs.js",
  "typings": "types/cjs.d.ts",
  "exports": {
    ".": {
      "import": {
        "types": "./types/es.d.ts",
        "node": "./dist/main.node-es.mjs",
        "default": "./dist/main.browser-es.mjs"
      },
      "require": {
        "types": "./types/cjs.d.ts",
        "node": "./dist/main.node-cjs.js",
        "default": "./dist/main.browser-cjs.js"
      }
    }
  },
  "files": [
    "dist",
    "types"
  ],
  "homepage": "https://mega.js.org/",
  "repository": {
    "type": "git",
    "url": "https://github.com/qgustavor/mega.git"
  },
  "keywords": [
    "mega",
    "mega.io",
    "mega.nz",
    "mega.co.nz",
    "storage"
  ],
  "scripts": {
    "build": "node build",
    "test": "npm run lint && npm run test-runner",
    "test-runner": "node test/helpers/test-runner.mjs",
    "lint": "npm run lint-js && npm run lint-ts",
    "lint-fix": "npm run lint-js-fix && npm run lint-ts-fix",
    "lint-js": "standard",
    "lint-js-fix": "standard --fix",
    "lint-ts": "ts-standard",
    "lint-ts-fix": "ts-standard --fix",
    "dist": "npm run test && npm run build"
  },
  "standard": {
    "ignore": [
      "dist"
    ]
  },
  "ts-standard": {
    "files": "types/cjs.d.ts"
  },
  "author": "TÃµnis Tiigi <tonistiigi@gmail.com>",
  "contributors": [
    "Gustavo Rodrigues (https://qgustavor.tk)"
  ],
  "license": "MIT",
  "publishConfig": {
    "provenance": true
  },
  "dependencies": {
    "abort-controller": "^3.0.0",
    "node-fetch": "^2.6.7",
    "pumpify": "^2.0.1",
    "secure-random": "^1.1.2",
    "stream-skip": "^1.0.3"
  },
  "devDependencies": {
    "@types/node": "^17.0.14",
    "@types/node-fetch": "^2.5.12",
    "ava": "^5.1.0",
    "buffer": "^6.0.3",
    "core-js": "^3.20.3",
    "esbuild": "^0.14.11",
    "esbuild-plugin-alias": "^0.2.1",
    "events": "^3.3.0",
    "mega-mock": "^0.4.2",
    "readable-stream": "^3.6.0",
    "regenerator-runtime": "^0.13.9",
    "standard": "^17.0.0-2",
    "tmp-promise": "^3.0.3",
    "ts-standard": "^11.0.0",
    "util": "^0.12.4"
  }
}
